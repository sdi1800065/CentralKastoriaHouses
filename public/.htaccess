<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /

  # Keep direct requests to existing files/folders untouched.
  RewriteCond %{REQUEST_FILENAME} -f [OR]
  RewriteCond %{REQUEST_FILENAME} -d
  RewriteRule ^ - [L]

  # Send all other requests to the React entry point.
  RewriteRule ^ index.html [L]
</IfModule>
